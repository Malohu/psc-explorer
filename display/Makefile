####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

dist: 
	@$(CHK_DIR_EXISTS) .tmp/display1.0.0 || $(MKDIR) .tmp/display1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/display1.0.0/ && $(COPY_FILE) --parents ../commons/geometry.h ../commons/point.h DisplayClient.h ../commons/instruction.h ../commons/error.h .tmp/display1.0.0/ && $(COPY_FILE) --parents main.cpp ../commons/geometry.cpp ../commons/point.cpp DisplayClient.cpp ../commons/instruction.cpp ../commons/error.cpp .tmp/display1.0.0/ && (cd `dirname .tmp/display1.0.0` && $(TAR) display1.0.0.tar display1.0.0 && $(COMPRESS) display1.0.0.tar) && $(MOVE) `dirname .tmp/display1.0.0`/display1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/display1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

